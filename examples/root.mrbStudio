source="/mnt/backup"
destination_parent="/path/to/backup/directory/"
prefix="Marios.root"
mount_source_command="mount /dev/sdXY /mnt/backup"
umount_source_command="umount /mnt/backup"
mount_destination_parent_command=""
umount_destination_parent_command=""
destination_fstype=btrfs

EXCLUDE_START
/tmp/*
/var/tmp/*
/usr/src/*
EXCLUDE_STOP
